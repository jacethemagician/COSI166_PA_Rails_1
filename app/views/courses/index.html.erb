<p id="notice"><%= notice %></p>

<h1>Courses</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Term</th>
      <th>Code</th>
      <th>Name</th>
      <th>Continuity</th>
      <th>Description</th>
      <th>Credits</th>
      <th>Independent_study</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.id %></td>
        <td><%= course.term %></td>
        <td><%= course.code %></td>
        <td><%= course.name %></td>
        <td><%= course.continuity_id %></td>
        <td><%= course.description %></td>
        <td><%= course.credits %></td>
        <td><%= course.independent_study %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course', new_course_path %>
